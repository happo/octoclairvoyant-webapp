{
  "name": "octoclairvoyant-webapp",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "npm": "^8.0.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "next",
    "build": "next build",
    "serve": "next start",
    "smoketest": "npm run type-check && npm run lint && npm run format:check && npm run build",
    "type-check": "tsc --project tsconfig.validation.json",
    "type-check:cypress": "tsc --project cypress/tsconfig.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "prettier-base": "prettier . --ignore-path .gitignore --ignore-unknown",
    "format": "npm run prettier-base --write",
    "format:check": "npm run prettier-base --check",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:ci": "npm run test -- --ci",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test 'npm run build && npm run serve' http://localhost:3000 'npm run cypress'",
    "e2e:dev": "start-server-and-test 'npm run dev' http://localhost:3000 'npm run cypress'",
    "e2e:headless": "start-server-and-test 'npm run build && npm run serve' http://localhost:3000 'npm run cypress:headless'",
    "gen:theme-typings": "chakra-cli tokens src/customTheme.ts --strict-component-types",
    "prepare": "is-ci || husky install",
    "postinstall": "npm run gen:theme-typings"
  },
  "dependencies": {
    "@chakra-ui/react": "1.8.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@octokit/auth-callback": "2.0.1",
    "@octokit/rest": "18.12.0",
    "@sentry/nextjs": "6.17.3",
    "downshift": "6.1.7",
    "focus-visible": "5.2.0",
    "framer-motion": "5.6.0",
    "lodash": "4.17.21",
    "next": "12.0.9",
    "next-seo": "4.29.0",
    "nookies": "2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "react-query": "3.34.8",
    "rehype-highlight": "5.0.2",
    "rehype-react": "7.0.4",
    "remark-gfm": "3.0.1",
    "remark-github": "11.2.2",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "remark-stringify": "10.0.2",
    "semver": "7.3.5",
    "unified": "10.1.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "1.7.0",
    "@octokit/openapi-types": "11.2.0",
    "@testing-library/cypress": "8.0.2",
    "@types/happo-cypress": "1.17.2",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@types/node": "14.18.9",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/semver": "7.3.9",
    "@types/unist": "2.0.6",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "cypress": "9.3.1",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-unicorn": "40.1.0",
    "happo-cypress": "3.0.1",
    "happo-e2e": "1.0.3",
    "happo.io": "7.0.1",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.4.7",
    "lint-staged": "12.3.2",
    "prettier": "2.5.1",
    "start-server-and-test": "1.14.0",
    "type-fest": "2.11.0",
    "typescript": "4.5.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
